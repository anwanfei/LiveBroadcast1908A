<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_get_en"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="get异步"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_post_en"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="post异步"
        app:layout_constraintTop_toBottomOf="@id/btn_get_en" />

    <Button
        android:id="@+id/btn_post_string"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="psot-string"
        app:layout_constraintTop_toBottomOf="@id/btn_post_en" />

    <Button
        android:id="@+id/btn_post_json"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="post-json"
        app:layout_constraintTop_toBottomOf="@id/btn_post_string" />

    <Button
        android:id="@+id/btn_psot_stream"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="stream"
        app:layout_constraintTop_toBottomOf="@id/btn_post_json" />

    <Button
        android:id="@+id/btn_project"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="项目"
        app:layout_constraintTop_toBottomOf="@id/btn_psot_stream" />

    <Button
        android:id="@+id/btn_redpackage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="红包"
        app:layout_constraintTop_toBottomOf="@id/btn_project" />

</androidx.constraintlayout.widget.ConstraintLayout>